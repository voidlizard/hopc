($ foreign "eq" eq? ((:int :int) :bool) $)
($ foreign "display" display ((:string) :unit) $)
($ foreign "random" random ((:unit) :init) $)

(define (test) (if (eq? (random) 1 ) 2 (if (eq? (random) 2 ) 3 0) ) )

($ typeof main ((:unit) :unit) $)

(define (main)

    (if (eq? (test) 2)
        (display "foo")
        (display "bar")
    )

    (if (eq? (test) 3)
        (display "piu")
        (display "paff")
    )

    (main)

)

(main)

